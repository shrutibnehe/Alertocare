        xmlns:local="clr-namespace:AlertToCareUI.Windows"
        xmlns:views="clr-namespace:AlertToCareUI.Views"
        mc:Ignorable="d"
        Title="ICU1Beds12Window" Height="600" Width="900" WindowState="Maximized">
